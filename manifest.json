{
    "manifest_version": 3,
    "name": "Crypto Prices",
    "version": "1.1",
    "description": "Ver el precio de criptomonedas principales y meme coins.",
    "permissions": ["storage"], 
    "host_permissions": ["https://api.coingecko.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "options_page": "options.html"
  }
  